{
  "name": "thinkord-poc",
  "version": "0.1.0",
  "description": "Menu display bar or restaurant",
  "author": "timlo",
  "build": {
    "appId": "react.electron"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/node": "^14.11.2",
    "bootstrap": "^4.5.2",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "webpack --config ./webpack.config.js",
    "build": "npm run electron-build && npm run react-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder --config electron-builder.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "electron": "^9.1.2",
    "electron-builder": "^22.8.0",
    "typescript": "^4.0.3",
    "wait-on": "^5.1.0",
    "webpack-cli": "^3.3.12"
  }
}
